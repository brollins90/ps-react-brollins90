{
    "name": "ps-react-brollins90",
    "version": "0.1.0",
    "private": true,
    "homepage": "http://brollins90.github.io/ps-react-brollins90/",
    "scripts": {
        "build:commonjs": "cross-env NODE_ENV=production babel ./src/components --out-dir ./lib --ignore spec.js",
        "build:css": "cpx \"./src/components/**/*.css\" ./lib",
        "build:docs": "node scripts/build.js",
        "build:lib": "npm run build:commonjs && npm run build:css",
        "deploy:docs": "gh-pages -d build",
        "gen:docs": "node scripts/generateComponentData.js",
        "gen:docs-watch": "npm run gen:docs -- --watch",
        "prebuild:lib": "rimraf lib",
        "predeploy:docs": "npm run build:docs",
        "prestart": "npm run gen:docs",
        "start": "npm-run-all --parallel start:docs gen:docs-watch",
        "start:docs": "node scripts/start.js",
        "test": "node scripts/test.js --env=jsdom"
    },
    "dependencies": {
        "autoprefixer": "7.1.2",
        "babel-core": "6.25.0",
        "babel-eslint": "7.2.3",
        "babel-jest": "20.0.3",
        "babel-loader": "7.1.1",
        "babel-preset-react-app": "3.0.1",
        "babel-runtime": "6.23.0",
        "case-sensitive-paths-webpack-plugin": "2.1.1",
        "chalk": "1.1.3",
        "css-loader": "0.28.4",
        "dotenv": "4.0.0",
        "eslint": "3.19.0",
        "eslint-config-react-app": "1.0.5",
        "eslint-loader": "1.9.0",
        "eslint-plugin-flowtype": "2.35.0",
        "eslint-plugin-import": "2.7.0",
        "eslint-plugin-jsx-a11y": "5.0.3",
        "eslint-plugin-react": "7.1.0",
        "extract-text-webpack-plugin": "2.1.2",
        "file-loader": "0.11.2",
        "fs-extra": "3.0.1",
        "highlight.js": "9.12.0",
        "html-webpack-plugin": "2.29.0",
        "jest": "20.0.4",
        "object-assign": "4.1.1",
        "postcss-flexbugs-fixes": "3.0.0",
        "postcss-loader": "2.0.6",
        "promise": "7.1.1",
        "react": "15.6.1",
        "react-dev-utils": "3.0.2",
        "react-dom": "15.6.1",
        "react-error-overlay": "1.0.9",
        "style-loader": "0.18.2",
        "styled-components": "1.4.5",
        "sw-precache-webpack-plugin": "0.11.4",
        "url-loader": "0.5.9",
        "webpack": "2.6.1",
        "webpack-dev-server": "2.5.1",
        "webpack-manifest-plugin": "1.1.2",
        "whatwg-fetch": "2.0.3"
    },
    "devDependencies": {
        "babel-cli": "6.24.1",
        "chokidar": "1.7.0",
        "cpx": "1.5.0",
        "cross-env": "5.0.1",
        "enzyme": "2.9.1",
        "gh-pages": "0.12.0",
        "jest-cli": "20.0.4",
        "npm-run-all": "4.0.2",
        "prop-types": "15.5.10",
        "react-addons-test-utils": "15.6.0",
        "react-docgen": "2.16.0",
        "react-test-renderer": "15.6.1",
        "rimraf": "2.6.1"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx}"
        ],
        "setupFiles": [
            "<rootDir>/config/polyfills.js"
        ],
        "testMatch": [
            "<rootDir>/src/**/__tests__/**/*.js?(x)",
            "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
        ],
        "testEnvironment": "node",
        "testURL": "http://localhost",
        "transform": {
            "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
        ],
        "moduleNameMapper": {
            "^react-native$": "react-native-web"
        },
        "moduleFileExtensions": [
            "web.js",
            "js",
            "json",
            "web.jsx",
            "jsx"
        ]
    },
    "babel": {
        "presets": [
            "react-app"
        ]
    },
    "eslintConfig": {
        "extends": "react-app"
    }
}
